{
  "apps" : [{
    "name": "mocam.server.stg",
    "script": "src/index.js",
    "ignore_watch" : ["node_modules", "logs"],
    "watch_options": {
      "followSymlinks": false
    },
    "env": {
      "NODE_ENV": "staging",
      "NODE_PATH": "src"
    },
    "merge_logs": true,
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "error_file": "./logs/mocam.server.stg.stderr.log",
    "out_file": "./logs/mocam.server.stg.stdout.log",
    "pid_file": "./logs/mocam.server.stg.pid"
  }]
}